{
  "name": "Générateur de Cas de Test",
  "description": "Application pour générer des cas de test à partir de user stories",
  "key": "test-case-generator",
  "baseUrl": "https://qalilab-ai.onrender.com",
  "vendor": {
    "name": "Amani Consulting",
    "url": "https://qalilab-ai.onrender.com"
  },
  "authentication": {
    "type": "none"
  },
  "lifecycle": {
    "installed": "/installed",
    "uninstalled": "/uninstalled"
  },
  "scopes": [
    "read"
  ],
  "apiVersion": 1,
  "modules": {
    "webItems": [
      {
        "key": "generate-test-case-button",
        "name": {
          "value": "Générer un cas de test"
        },
        "location": "jira.issue.tools",
        "url": "/jira-test-generator?issueKey={issue.key}&returnUrl={issue.url}",
        "conditions": [
          {
            "condition": "entity_property_equal_to",
            "params": {
              "entity": "issue",
              "propertyKey": "issuetype.name",
              "objectName": "Story"
            }
          }
        ],
        "tooltip": {
          "value": "Générer un cas de test basé sur cette user story"
        }
      }
    ],
    "jiraIssueContents": [
      {
        "key": "test-generator-panel",
        "name": {
          "value": "Générateur de cas de test"
        },
        "url": "/jira-test-generator?issueKey={issue.key}&returnUrl={issue.url}",
        "icon": {
          "width": 16,
          "height": 16,
          "url": "/images/test-icon.png"
        },
        "conditions": [
          {
            "condition": "entity_property_equal_to",
            "params": {
              "entity": "issue",
              "propertyKey": "issuetype.name",
              "objectName": "Story"
            }
          }
        ]
      }
    ]
  }
}