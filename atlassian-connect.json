{
  "name": "Générateur de Cas de Test",
  "description": "Génère automatiquement des cas de test à partir des user stories",
  "key": "test-generator-app",
  "baseUrl": "https://qalilab-ai.onrender.com",
  "vendor": {
    "name": "QALILAB-AI",
    "url": "https://qalilab-ai.onrender.com"
  },
  "authentication": {
    "type": "jwt"
  },
  "apiVersion": 1,
  "modules": {
    "webPanels": [
      {
        "key": "test-generator-panel",
        "name": {
          "value": "Générer des tests"
        },
        "location": "atl.jira.view.issue.right.context",
        "url": "/jira-test-generator?issueKey={issue.key}",
        "weight": 100,
        "conditions": [
          {
            "condition": "user_is_logged_in"
          },
          {
            "condition": "issue_type_is",
            "params": {
              "issueType": "Story"
            }
          }
        ]
      }
    ],
    "webItems": [
      {
        "key": "test-generator-main-button",
        "name": {
          "value": "Générer des tests"
        },
        "location": "operations-dropdown",
        "url": "/jira-test-generator?issueKey={issue.key}",
        "weight": 10,
        "conditions": [
          {
            "condition": "user_is_logged_in"
          },
          {
            "condition": "issue_type_is",
            "params": {
              "issueType": "Story"
            }
          }
        ]
      },
      {
        "key": "test-generator-dropdown",
        "name": {
          "value": "Générer des tests"
        },
        "location": "more-dropdown-items",
        "url": "/jira-test-generator?issueKey={issue.key}",
        "weight": 10,
        "conditions": [
          {
            "condition": "user_is_logged_in"
          },
          {
            "condition": "issue_type_is",
            "params": {
              "issueType": "Story"
            }
          }
        ]
      }
    ],
    "jiraEntityProperties": [
      {
        "key": "test-generator-property",
        "name": {
          "value": "Propriété générateur de test"
        },
        "entityType": "issue"
      }
    ]
  },
  "lifecycle": {
    "installed": "https://qalilab-ai.onrender.com/installed",
    "uninstalled": "https://qalilab-ai.onrender.com/uninstalled"
  },
  "scopes": [
    "READ",
    "WRITE",
    "ACT_AS_USER",
    "ADMIN"
  ]
}